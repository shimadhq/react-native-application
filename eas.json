{
    "cli": {
      "version": ">= 0.52.0"
    },
    "build": {
      "development": {
        "developmentClient": true,
        "distribution": "internal",
        "ios": {
        "simulator": true
        },
        "android": {
          "buildType": "apk"
        }
      },
      "preview": {
        "distribution": "internal",
        "android": {
        "buildType": "apk"
        }, 
        "ios": {
          "simulator": false,
          "resourceClass": "medium"
        }
      },
      "production": {
        "distribution": "store",
        "channel": "production",
        "ios": {
        "resourceClass": "medium"
        },
        "android": {
          "buildType": "aab"
        }
      }
    },
    "submit": {
      "production": {}
    }
}